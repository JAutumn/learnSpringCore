<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="saveTarget" class="com.simbirsoft.SaveTarget"/>

    <bean id="t800" class="com.simbirsoft.T800">
        <constructor-arg ref="saveTarget"/>
    </bean>

    <bean id="killTarget" class="com.simbirsoft.KillTarget"/>

    <bean id="t1000" class="com.simbirsoft.T1000">
        <property name="target" ref="killTarget"/>
    </bean>
</beans>